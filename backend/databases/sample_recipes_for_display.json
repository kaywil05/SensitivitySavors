      },
      "dairy_free": true,
      "vegetarian": true,
      "vegan": true
    }
  ],
  "ingredients": [
    {
      "_id": {
        "$oid": "661f7f2c5e60845f94eb252a"
      },
      "name": "thinly sliced cucumbers",
      "quantity": "2",
      "unit": "cups",
      "note": "Japanese, English, or regular"
    },
    {
      "_id": {
        "$oid": "661f7f2c5e60845f94eb252b"
      },
      "name": "salt",
      "quantity": "1/2 - 1",
      "unit": "teaspoon",
      "note": "to taste"
    },
    {
      "_id": {
        "$oid": "661f7f2c5e60845f94eb252c"
      },
      "name": "pepper",
      "quantity": "1/4",
      "unit": "teaspoon",
      "note": ""
    },
    {
      "_id": {
        "$oid": "661f7f2c5e60845f94eb252d"
      },
      "name": "sugar",
      "quantity": "1",
      "unit": "tablespoon",
      "note": "or slightly less honey"
    },
    {
      "_id": {
        "$oid": "661f7f2c5e60845f94eb252e"
      },
      "name": "soy sauce",
      "quantity": "1",
      "unit": "tablespoon",
      "note": ""
    },
    {
      "_id": {
        "$oid": "661f7f2c5e60845f94eb252f"
      },
      "name": "rice wine vinegar",
      "quantity": "1/4",
      "unit": "cup",
      "note": ""
    },
    {
      "_id": {
        "$oid": "661f7f2c5e60845f94eb2530"
      },
      "name": "sesame oil",
      "quantity": "1",
      "unit": "teaspoon",
      "note": ""
    },
    {
      "_id": {
        "$oid": "661f7f2c5e60845f94eb2531"
      },
      "name": "sesame seeds",
      "quantity": "2",
      "unit": "teaspoons",
      "note": ""
    }
  ],
  "instructions": [
    {
      "_id": {
        "$oid": "661f7f2c5e60845f94eb2532"
      },
      "step": "If using English or regular cucumbers, they need to be peeled, cut lengthwise, seeded and sliced into thin half moons."
    },
    {
      "_id": {
        "$oid": "661f7f2c5e60845f94eb2533"
      },
      "step": "In a large bowl, combine cucumbers, salt and pepper, mix well."
    },
    {
      "_id": {
        "$oid": "661f7f2c5e60845f94eb2534"
      },
      "step": "Add sugar(or honey), soy sauce, rice wine vinegar and sesame oil and mix well."
    },
    {
      "_id": {
        "$oid": "661f7f2c5e60845f94eb2535"
      },
      "step": "Sprinkle with sesame seeds and mix."
    },
    {
      "_id": {
        "$oid": "661f7f2c5e60845f94eb2536"
      },
      "step": "Let marinate for 20 minutes before serving."
    },
    {
      "_id": {
        "$oid": "661f7f2c5e60845f94eb2537"
      },
      "step": "Adjust seasonings before serving."
    }
  ],
