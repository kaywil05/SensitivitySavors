  "name": "5 Ingredient Black Bean Soup",
  "author": "Gimme Some Oven",
  "prep_time": "2 minutes",
  "cook_time": "18 minutes",
  "total_time": "20 minutes",
  "servings": 4,
  "category": [
    {
      "_id": {
        "$oid": "661f825f5e60845f94eb2557"
      },
      "dairy_free": true,
      "gluten_free": true,
      "vegetarian": true,
      "vegan": true
    }
  ],
  "ingredients": [
    {
      "_id": {
        "$oid": "661f825f5e60845f94eb2558"
      },
      "name": "black beans",
      "quantity": "3 (15 oz)",
      "unit": "cans ",
      "note": "with liquid"
    },
    {
      "_id": {
        "$oid": "661f825f5e60845f94eb2559"
      },
      "name": "good-quality salsa",
      "quantity": "1",
      "unit": "lb",
      "note": "about 2.5 cups"
    },
    {
      "_id": {
        "$oid": "661f825f5e60845f94eb255a"
      },
      "name": "chopped fresh cilantro",
      "quantity": "1/2",
      "unit": "cup",
      "note": "loosely packed, plus extra for garnish"
    },
    {
      "_id": {
        "$oid": "661f825f5e60845f94eb255b"
      },
      "name": "ground cumin",
      "quantity": "2",
      "unit": "teaspoon",
      "note": ""
    },
    {
      "_id": {
        "$oid": "661f825f5e60845f94eb255c"
      },
      "name": "garlic",
      "quantity": "1",
      "unit": "clove ",
      "note": "minced"
    }
  ],
